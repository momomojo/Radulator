  "clinicalInterpretation": {
    "description": "Guidelines for interpreting measured spleen sizes relative to calculated ULN",
    "categories": [
      {
        "category": "Normal",
        "criteria": "Measured spleen length <= calculated ULN",
        "note": "Within 95th percentile for height and sex"
      },
      {
        "category": "Borderline Enlarged",
        "criteria": "Measured length 1-2 cm above calculated ULN",
        "note": "Mild splenomegaly relative to patient's body habitus"
      },
      {
        "category": "Mildly Enlarged",
        "criteria": "Measured length 2-4 cm above calculated ULN OR 13-15 cm absolute",
        "note": "Consider clinical context and etiology"
      },
      {
        "category": "Moderately Enlarged",
        "criteria": "Measured length 4-8 cm above calculated ULN OR 15-20 cm absolute",
        "note": "Significant splenomegaly warranting workup"
      },
      {
        "category": "Massively Enlarged",
        "criteria": "Measured length >8 cm above calculated ULN OR >20 cm absolute",
        "note": "Associated with hematologic malignancies, portal hypertension, storage diseases"
      }
    ]
  },
  "referenceValues": {
    "traditionalFixedThreshold": {
      "normalUpperLimit": "12 cm",
      "limitations": "Does not account for height or sex differences; 26% of healthy men exceeded this limit in Chow et al. study"
    },
    "heightAdjustedThresholds": {
      "shortFemale155cm": "11.9 cm",
      "averageFemale165cm": "12.2 cm",
      "tallFemale180cm": "12.6 cm",
      "shortMale165cm": "12.6 cm",
      "averageMale175cm": "13.2 cm",
      "tallMale200cm": "14.5 cm"
    },
    "volumeThresholds": {
      "massiveSplenomegaly": ">943 cm3 (5 SD above mean)",
      "clinicallyRelevant": ">300 cm3 typically considered enlarged"
    }
  },
  "testValidation": {
    "inputFields": [
