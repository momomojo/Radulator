        "unit": "cm",
        "validatedRanges": {
          "female": "155-180",
          "male": "165-200"
        },
        "required": true
      }
    ],
    "outputFields": [
      {
        "field": "Spleen length should not exceed (cm)",
        "format": "decimal to 1 place"
      },
      {
        "field": "Spleen volume should not exceed (cm3)",
        "format": "rounded integer"
      },
      {
        "field": "Interpretation",
        "format": "text string"
      },
      {
        "field": "Warning",
        "format": "text string (only if outside validated range)"
      }
    ]
  }
